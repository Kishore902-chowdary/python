  --->
  #factorial of a number
def fact1(a):
  fact=1
  for i in range(1,a+1):
    fact*=i
  return fact
def fact2(a):
  fact=1
  for i in range(1,a+1):
    fact*=i
  print(fact)
def fact3():
  a=int(input())
  fact=1
  for i in range(1,a+1):
    fact*=i
  print(fact)
def fact4():
  a=int(input())
  fact=1
  for i in range(1,a+1):
    fact*=i
  return fact




k=int(input())
f1=fact1(k)
print(f1)
fact2(k)
fact3()
f4=fact4()
print(f4)


-->
#default parameters
def area_ci(pi=2,radius=4):   #formal parameters(parameter with in function)
  print(pi,radius)
area_ci()
area_ci(10)
area_ci(3,4)
area_ci(radius=3,pi=4)     #actual parameters(parameter while calling function)
area_ci(20,radius=10) 


--->
#local scope,global scope
a=10      #global
def dis():
  a=20     #local
  print(a)
dis()


--->
#recursive functions
def fibonacci(a,b):
  c=a+b
  if(c<=n):
    print(c,end=" ")
    a=b
    b=c
    fibonacci(a,b)
n=int(input("Enter the number n "))
a=0
b=1
print(a,b,end=" ")
fibonacci(a,b)

