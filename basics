#5

sales=float(input("Enter the sales"))
basic=4000
DA=(basic/100)*110
conv=500
if(sales>=100000):
  HRA=((basic)/100)*10
  incent=(sales/100)*10
  bonus=1000
  print(f"salary={basic+conv+HRA+incent+bonus+DA}")
else:
  HRA=((basic)/100)*20
  incent=(sales/100)*4
  bonus=500
  print(f"salary={basic+conv+HRA+incent+bonus+DA}")
  
