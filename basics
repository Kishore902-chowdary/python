# str.lower(), str.upper, str.isupper(), str.islower()
temp=input("Enter the temperature").lower()
hum=input("Enter the humidity").lower()
if(temp=="warm"):
  if(hum=="dry"):
    print("Play Basketball")
  elif(hum=="humid"):
    print("Play Tennis")
elif(temp=="cold"):
  if(hum=="dry"):
    print("Play Cricket")
  elif(hum=="humid"):
    print("Swim")
else:
  print("Enter valid temp and humid")
  
