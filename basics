--->
a=int(input("Start num range"))
b=int(input("End num range"))
for i in range(a,b+1):
  count=0
  for j in range(1,i+1):
    if(i%j==0):
      count=count+1
  
  if(count==2):
    print(f"{i} is prime")
  else:
    print(f"{i} is not prime")


