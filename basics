#immutiable(tuple,int,string ,all the data types)

a=10      #first a is deleted by using garbage collector
print(id(a))
a=a+10
print(id(a))


--->
#add method in set
a={1,2,3,4,5}
a.add(25)
a.add(9)
print(a)

--->
#clear method in set
a={1,2,3,4,5}
a.clear()
print(a)

--->
#remove method in set
a={1,2,3,4,5}
a.remove(2)
print(a)

--->
#subset metthod in set
a={1,2,3,4,5}
b={2,3,4}
print(b.issubset(a))

--->
a="hell"
print(a.ljust(30,'-'))
print(a.rjust(30,'-'))
print(a.center(30,'-'))


