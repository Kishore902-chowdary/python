suma=float(input())
if(Arjun>suma):
  print("Arjun is taller than Suma")
  print(f"Arjun height is {format(Arjun*0.3937,'.2f')}")
  print(f"Suma height is {format(suma*0.3937,'.2f')}")
else:
  print("suma is taller than Arjun")
  print(f"Arjun height is {format(Arjun*0.3937,'.2f')}")
  print(f"Suma height is {format(suma*0.3937,'.2f')}")

