  # nested if(if inside if)

a=int(input("Enter the number a"))
b=int(input("Enter thr number b"))
c=int(input("Enter the number c"))
if(a>b):
  if(a>c):
    print("a is grater")
  else:
    print("c is grater")
elif(b>c):
  print("b is grater")
elif(c>b):
  print("c is greater")
else:
  print("All are equal")


