--->
#methods    prameter as(*arg)
def add(a,*ar):
  sum=0
  sum=sum+a
  for i in ar:
    sum=sum+i
  return sum
p=add(10,20,30,40)
print(p)


-->
# zip function
a=(("hi",100),("hello",200))
b,c=zip(a)
print(b)
print(c)
a=(("hi",100),("hello",200))
b,c=zip(*a)
print(b)
print(c)

